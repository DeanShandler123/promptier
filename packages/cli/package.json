{
  "name": "@promptier/cli",
  "version": "0.2.1",
  "description": "CLI for promptier - trace your prompts",
  "type": "module",
  "bin": {
    "promptier": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@promptier/core": "^0.2.0",
    "@promptier/lint": "^0.2.0",
    "commander": "^12.0.0",
    "jiti": "^2.4.0",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "llm",
    "prompt",
    "cli",
    "ai"
  ],
  "author": "Dean Shandler",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DeanShandler123/promptier.git",
    "directory": "packages/cli"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/DeanShandler123/promptier#readme",
  "bugs": {
    "url": "https://github.com/DeanShandler123/promptier/issues"
  }
}
